@article{Penney74,
author = {C. M. Penney and R. L. St. Peters and M. Lapp},
journal = {J. Opt. Soc. Am.},
keywords = {Backscattering; Depolarization; Polarization; Raman scattering; Rayleigh scattering; Refractive index},
number = {5},
pages = {712--716},
publisher = {Optica Publishing Group},
title = {Absolute rotational Raman cross sections for N2, O2, and CO2},
volume = {64},
month = {May},
year = {1974},
url = {https://opg.optica.org/abstract.cfm?URI=josa-64-5-712},
doi = {10.1364/JOSA.64.000712},
abstract = {We present absolute intensities for rotational Raman scattering (RRS) from N2, O2, and CO2, excited at 488.0 and 647.1 nm. The absolute scattering intensity for N2 at 488.0 nm is characterized by its differential cross section for backscattering, summed over Stokes and anti-Stokes bands and over scattered-light polarizations, which we find to be 1.64 {\texttimes} 10{\textminus}29 cm2/sr {\textpm}8\%. The ratio of the cross section for O2 to that for N2 at 488.0 nm is 2.61 {\textpm} 5\%, whereas the corresponding ratio for CO2 to N2 is 10.6 {\textpm} 10\%. Our values for RRS cross sections relative to the N2 vibrational Raman cross section are in reasonable agreement with corresponding ratios reported recently by Fenner et al. On the other hand, our absolute cross sections are approximately twice as large as those obtained from the results of Fenner et al., but agree closely with values calculated from recent measurements of the depolarization of Rayleigh scattering. Detailed observations of relative rotational-Raman-line intensities at temperatures of 22, 75, and 125 {\textdegree}C are consistent with theoretical predictions.},
}

